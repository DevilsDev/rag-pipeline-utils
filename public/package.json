{
  "name": "rag-evaluation-dashboard",
  "version": "1.0.0",
  "description": "Interactive evaluation dashboard for RAG Pipeline Utils - analyze BLEU scores, ROUGE metrics, and model performance",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 3000",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"**/*.{js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "vite-bundle-analyzer"
  },
  "dependencies": {
    "chart.js": "^4.4.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6",
    "terser": "^5.24.0",
    "vite": "^5.0.8",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "keywords": [
    "rag",
    "evaluation",
    "dashboard",
    "bleu",
    "rouge",
    "ai",
    "machine-learning",
    "react",
    "vite",
    "tailwindcss"
  ],
  "author": {
    "name": "Ali Kahwaji",
    "email": "ali@devilsdev.com",
    "url": "https://github.com/alikahwaji"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DevilsDev/rag-pipeline-utils.git",
    "directory": "public"
  },
  "bugs": {
    "url": "https://github.com/DevilsDev/rag-pipeline-utils/issues"
  },
  "homepage": "https://devilsdev.github.io/rag-pipeline-utils/",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
