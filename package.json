{
  "name": "@devilsdev/rag-pipeline-utils",
  "version": "2.1.7",
  "description": "A comprehensive enterprise-grade toolkit for building RAG (Retrieval-Augmented Generation) pipelines with advanced AI/ML capabilities",
  "main": "dist/index.cjs",
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./cli": "./bin/cli.js"
  },
  "bin": {
    "rag-pipeline": "./bin/cli.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "prepack": "npm run build && npm test",
    "prepare": "npm run build",
    "guard:cjs": "node scripts/guard-cjs.js",
    "pretest": "npm run guard:cjs && node scripts/setup.js",
    "test": "jest --runInBand --experimental-vm-modules",
    "test:ci": "jest --experimental-vm-modules --coverage",
    "posttest": "node scripts/generate-test-reports.js",
    "format": "prettier --write \"src/**/*.{js,json}\"",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .js --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --ignore-path .gitignore --fix",
    "lint:strict": "eslint . --ext .js,.jsx --ignore-path .gitignore --max-warnings 0",
    "lint:errors-only": "eslint . --ext .js,.jsx --ignore-path .gitignore --quiet",
    "lint:diagnostic": "eslint . --ext .js,.jsx --ignore-path .gitignore --format=stylish",
    "lint:strict:ts": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:diagnostic:ts": "eslint . --ext .js,.jsx,.ts,.tsx --format=stylish",
    "lint:errors-only:ts": "eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint-staged": "lint-staged",
    "postinstall": "husky install",
    "verify:mocks": "node scripts/verify-fixture-mocks.js",
    "build:docs": "npx docusaurus build docs-site",
    "ci": "node scripts/ci-runner.js",
    "docs:dev": "npx docusaurus start docs-site",
    "docs:version": "npx docusaurus docs:version",
    "watch:lqip": "node docs-site/scripts/watch-images.js",
    "docs:clear": "docusaurus clear --site-dir docs-site",
    "docs:start": "docusaurus start docs-site",
    "docs:build": "docusaurus build docs-site",
    "docs:serve": "docusaurus serve docs-site",
    "docs:deploy": "docusaurus deploy docs-site",
    "roadmap:close": "node scripts/close-done-roadmap-issues.js",
    "labels:sync": "node scripts/ensure-roadmap-labels.js",
    "release:tag": "node scripts/generate-release-note.js v$(node -p \"require('./package.json').version\")",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit:critical": "npm audit --audit-level=critical --production || echo 'Development vulnerabilities found but acceptable'",
    "security:audit:json": "npm audit --json > security-audit-report.json || true",
    "security:fix": "npm audit fix",
    "security:fix:force": "npm audit fix --force",
    "security:check": "npm audit --production && echo 'Production security check passed'",
    "security:report": "npm run security:audit:json && echo 'Security report generated: security-audit-report.json'",
    "ci:hardening": "node scripts/ci/harden-workflows.js",
    "ci:audit": "node scripts/audit/gha-audit.js",
    "ci:targeted-fixes": "node scripts/ci/targeted-security-fixes.js",
    "ci:comprehensive-hardening": "node scripts/ci/comprehensive-hardening.js",
    "ci:shell-security": "node scripts/ci/shell-security-hardening.js",
    "ci:final-fixes": "node scripts/ci/final-shell-injection-fixes.js",
    "ci:test-workflows": "node scripts/ci/test-workflows.js",
    "ci:fix-remaining": "node scripts/ci/fix-remaining-actions.js",
    "tests:categorize": "node scripts/test-categorizer.js",
    "tests:affected": "node scripts/run-affected.js",
    "tests:full": "jest --runInBand --reporters=default --json --outputFile=test-results-current.json",
    "tests:ci": "jest --maxWorkers=50% --reporters=default --json --outputFile=test-results-current.json",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:simple": "jest --maxWorkers=1 --no-coverage",
    "verify:pack": "node scripts/verify-npm-pack.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DevilsDev/rag-pipeline-utils.git"
  },
  "keywords": [
    "rag",
    "retrieval-augmented-generation",
    "vectorstore",
    "llm",
    "pipeline",
    "cli",
    "openai"
  ],
  "author": "Ali Kahwaji",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DevilsDev/rag-pipeline-utils/issues"
  },
  "homepage": "https://github.com/DevilsDev/rag-pipeline-utils#readme",
  "files": [
    "dist/",
    "bin/",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix --quiet --no-warn-ignored"
    ],
    "bin/**/*.js": [
      "eslint --fix --quiet --no-warn-ignored"
    ],
    "*": "prettier --write"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/preset-classic": "^3.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.3",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/jest": "^29.5.14",
    "acorn": "8.15.0",
    "babel-jest": "^29.7.0",
    "codecov": "^3.8.2",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.2",
    "eslint": "^8.57.1",
    "eslint-plugin-storybook": "^0.12.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "prop-types": "^15.8.1",
    "rimraf": "^6.0.1",
    "semantic-release": "^21.1.2",
    "ts-jest": "^29.3.2",
    "why-is-node-running": "^3.2.2"
  },
  "dependencies": {
    "@octokit/rest": "^21.1.1",
    "ajv": "^8.17.1",
    "axios": "^1.8.4",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.5.0",
    "fast-glob": "^3.3.3",
    "framer-motion": "^12.7.4",
    "inquirer": "^8.2.5",
    "jsdom": "^26.1.0",
    "octokit": "^4.1.3",
    "openai": "^4.93.0",
    "pino": "^9.6.0",
    "plaiceholder": "^3.0.0",
    "sharp": "^0.34.1"
  }
}
